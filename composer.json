{
    "name": "estina/generator-bundle",
    "description": "Extension for SensioGeneratorBundle which provides ability to override skeleton templates by putting it to app/Resources/EstinaGenerator.",
    "authors": [
        {
            "name": "Zilvinas Kuusas",
            "email": "zilvinas.k@estina.lt"
        }
    ],
    "keywords": ["generator", "symfony2", "doctrine"],
    "homepage": "http://github.com/Estina/GeneratorBundle",
    "type": "library",
    "require": {
        "php"                                 : ">=5.3.0",
        "doctrine/doctrine-bundle"            : "1.2.0",
        "sensio/generator-bundle"             : "dev-master",
    },
    "autoload": {
        "psr-0": { "Estina\\GeneratorBundle": "" }
    },
    "target-dir": "Estina/GeneratorBundle",
    "minimum-stability": "dev",
}
