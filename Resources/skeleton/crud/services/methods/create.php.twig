
    /**
     * Persist newly created entity
     *
     * @param {{ entity }} $entity
     * @return bool
     */
    public function create({{ entity }} $entity)
    {
        $entity->setDateCreated(new \DateTime('now'));
        $entity->setDateUpdated(new \DateTime('now'));
        $this->em->persist($entity);

        return true;
    }